{
  "name": "hexboy/laravel-woocommerce-api-client",
  "description": "Laravel 5 wrapper for the Woocommerce REST API",
  "keywords": [
    "laravel",
    "wordpress",
    "woocommerce",
    "rest",
    "api",
    "ecommerce",
    "e-commerce",
    "shop",
    "cart"
  ],
  "homepage": "https://github.com/hexboy/laravel-woocommerce-api-client",
  "license": "MIT",
  "authors": [
    {
      "name": "Hassan Yousefi",
      "email": "hexboy2011@gmail.com",
      "homepage": "https://hexboy.ir/",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : "^7.0",
    "automattic/woocommerce": "^3.0.0"
  },
  "autoload": {
    "psr-4": {
      "Hexboy\\Woocommerce\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hexboy\\Woocommerce\\Test\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "mockery/mockery": "^1.0",
    "satooshi/php-coveralls": "^1.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hexboy\\Woocommerce\\WoocommerceServiceProvider"
      ],
      "aliases": {
        "Woocommerce": "Hexboy\\Woocommerce\\Facades\\Woocommerce"
      }
    }
  }
}
